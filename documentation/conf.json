{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./"],
    "exclude": ["./node_modules/", "./views/", "./test/", "./out/", "./db", "./.nyc_output"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}