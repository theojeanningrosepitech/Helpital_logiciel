{
  "name": "Helpital",
  "version": "1.0.5",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "start": "electron --disable-gpu --disable-software-rasterizer .",
    "server": "node server/server",
    "software": "npm run start",
    "test": "mocha --recursive --exit",
    "delete_database": "rm -rf ./docker-helpital/postgres/db-data/*",
    "remake_database": "remake_database.sh",
    "coverage": "nyc npm run test",
    "coverage-silent": "nyc --silent npm run test",
    "doc": "jsdoc -r -c ./documentation/conf.json -d ./documentation/docs && node ./documentation/docs.js",
    "build_project": "npx electron-packager . --platform=win32 --arch=x64 --out=helpital-sources Helpital --icon=./public/images/helpital_logo_heart.png --overwrite && node ./build_installer.js"
  },
  "dependencies": {
    "2fa-util": "^1.1.1",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chart.js": "^3.6.0",
    "concurrently": "^6.4.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "electron-dl": "^3.3.1",
    "express": "~4.16.1",
    "file-saver": "^2.0.5",
    "gridstack": "^7.1.1",
    "http-errors": "~1.6.3",
    "ics": "^2.35.0",
    "image-size": "^1.0.1",
    "jsdoc": "^3.6.11",
    "mocha": "^9.1.3",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.2",
    "nyc": "^15.1.0",
    "pg": "^8.7.1",
    "pug": "2.0.0-beta11",
    "qrcode": "^1.5.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.74.0",
    "uuid": "^8.3.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "electron": "^16.0.2",
    "electron-packager": "^15.4.0",
    "electron-winstaller": "^5.0.0",
    "electron-wix-msi": "^3.2.0"
  }
}
